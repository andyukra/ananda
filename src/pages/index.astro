---
import MainLayout from '../layouts/mainLayout.astro';
import Buzos from '../components/diapos/Buzos.svelte';
import Conjunto from '../components/diapos/Conjunto.svelte';
import Calzas from '../components/diapos/Calzas.svelte';
import Medias from '../components/diapos/Medias.svelte';

// NECESARIOS
import Diapo1 from '../components/diapos/Diapo1.svelte';
